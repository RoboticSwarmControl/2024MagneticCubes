file = {:C$\backslash$:/Users/Patrick/Documents/Mendeley Desktop/Demaine, Hendrickson, Lynch - 2018 - Toward a General Theory of Motion Planning Complexity Characterizing Which Gadgets Make Games Hard.pdf:pdf},
journal = {Leibniz International Proceedings in Informatics, LIPIcs},
keywords = {Computational complexity,EXP,Games,Motion planning,NEXP,NP,PSPACE,Undecidability},
month = {dec},
publisher = {Schloss Dagstuhl- Leibniz-Zentrum fur Informatik GmbH, Dagstuhl Publishing},
title = {{Toward a General Theory of Motion Planning Complexity: Characterizing Which Gadgets Make Games Hard}},
