isbn = {9783642453458},
issn = {16113349},
keywords = {Array permutations,Complexity,Nano-particles,Parallel motion planning,Robot swarm,Uniform inputs},
pages = {51--66},
title = {{Reconfiguring massive particle swarms with limited, global control}},
