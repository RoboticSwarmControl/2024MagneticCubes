arxivId = {1104.5226},
author = {Chen, Ho Lin and Doty, David},
booktitle = {Proceedings of the Twenty-Third Annual ACM-SIAM Symposium on Discrete Algorithms},
doi = {10.1137/151004161},
eprint = {1104.5226},
file = {:C$\backslash$:/Users/Patrick/Documents/Mendeley Desktop/Chen, Doty - 2011 - Parallelism and Time in Hierarchical Self-Assembly(2).pdf:pdf},
issn = {10957111},
keywords = {Assembly time,Chemical kinetics,Hierarchical,Parallelism,Self-assembly,Tile complexity},
pages = {1163--1182},
publisher = {Society for Industrial and Applied Mathematics Publications},
title = {{Parallelism and time in hierarchical self-assembly}},
